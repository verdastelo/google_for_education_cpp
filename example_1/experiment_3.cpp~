// experiment_3.cpp: Chetan Anand
// Description: A program that prints "Hello World!" (left-aligned) 4 times on a line for 6 lines, where each is printed in a field of 17 spaces.
// Google's solution: 

#include <iostream> 
using namespace std; 

#include <iostream>
#include <iomanip> 
using namespace std; 

int main() {
  for (int i = 0; i <= 6; i++) {
      for (int j = 0; j <= 4; j++) {
            cout << setw (17) << left << "Hello World! ";
      }
      cout << endl; 
  }
  return 0;
}
